{
  "name": "react_native_learning_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "link": "react-native link",
    "restart": "adb reverse tcp:8081 tcp:8081 && react-native start --reset-cache",
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "release": "npx jetify && cd android && ./gradlew assembleRelease && cd ../",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "ios:bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
